<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bckg"
    tools:context=".Folders">

    <Button
        android:id="@+id/saveButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Save Text"
        app:layout_constraintBottom_toTopOf="@+id/imageView2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.88"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/t6"
        app:layout_constraintVertical_bias="0.437"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/other"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Other Tree"

        app:layout_constraintBottom_toTopOf="@+id/imageView2"
        app:layout_constraintEnd_toStartOf="@+id/saveButton"
        app:layout_constraintHorizontal_bias="0.359"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/t5"
        app:layout_constraintVertical_bias="0.485"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="190dp"
        android:layout_height="90dp"
        android:fontFamily="@font/hammersmith_one"
        android:text="Report Visualization"
        android:textColor="@color/black"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/linearLayout6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.072"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.075" />


    <ImageButton
        android:id="@+id/backButton"
        android:layout_width="50dp"
        android:layout_height="51dp"
        android:background="@drawable/back"
        android:backgroundTint="@color/black"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.69"
        app:layout_constraintStart_toEndOf="@+id/textView"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.098"
        tools:ignore="MissingConstraints" />


    <ImageButton
        android:id="@+id/t1"
        android:layout_width="161dp"
        android:layout_height="127dp"
        android:background="@drawable/btn1"
        android:contentDescription="CERCOSPORA"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toTopOf="@+id/t3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.064"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        app:layout_constraintVertical_bias="0.64" />

    <View
        android:layout_width="120dp"
        android:layout_height="73dp"
        android:background="@drawable/login_white_backg5"
        app:layout_constraintBottom_toBottomOf="@+id/t1"
        app:layout_constraintEnd_toEndOf="@+id/t1"
        app:layout_constraintStart_toStartOf="@+id/t1"
        app:layout_constraintTop_toTopOf="@+id/t1"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/textView7"
        android:layout_width="90dp"
        android:layout_height="45dp"
        android:fontFamily="@font/hammersmith_one"
        android:gravity="center"
        android:inputType="text"
        android:hint="Tree 1"
        android:textAlignment="center"
        android:textAllCaps="true"
        android:textColor="#000000"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/t1"
        app:layout_constraintEnd_toEndOf="@+id/t1"
        app:layout_constraintStart_toStartOf="@+id/t1"
        app:layout_constraintTop_toTopOf="@+id/t1"
        app:layout_constraintVertical_bias="0.514" />


    <ImageButton
        android:id="@+id/t2"
        android:layout_width="161dp"
        android:layout_height="127dp"
        android:background="@drawable/btn2"
        android:contentDescription="CERCOSPORA"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toTopOf="@+id/t4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.657"
        app:layout_constraintStart_toEndOf="@+id/t1"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        app:layout_constraintVertical_bias="0.571" />

    <View
        android:layout_width="120dp"
        android:layout_height="73dp"
        android:background="@drawable/login_white_backg5"
        app:layout_constraintBottom_toBottomOf="@+id/t2"
        app:layout_constraintEnd_toEndOf="@+id/t2"
        app:layout_constraintHorizontal_bias="0.45"
        app:layout_constraintStart_toStartOf="@+id/t2"
        app:layout_constraintTop_toTopOf="@+id/t2"
        app:layout_constraintVertical_bias="0.467"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/textView8"
        android:layout_width="90dp"
        android:layout_height="45dp"
        android:fontFamily="@font/hammersmith_one"
        android:hint="Tree 2"
        android:gravity="center"
        android:inputType="text"
        android:textAlignment="center"
        android:textAllCaps="true"
        android:textColor="#000000"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/t2"
        app:layout_constraintEnd_toEndOf="@+id/t2"
        app:layout_constraintStart_toStartOf="@+id/t2"
        app:layout_constraintTop_toTopOf="@+id/t2"
        app:layout_constraintVertical_bias="0.514" />


    <ImageButton
        android:id="@+id/t3"
        android:layout_width="161dp"
        android:layout_height="127dp"
        android:layout_marginBottom="16dp"
        android:background="@drawable/btn3"
        android:contentDescription="CERCOSPORA"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toTopOf="@+id/t5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.064"
        app:layout_constraintStart_toStartOf="parent" />

    <View
        android:id="@+id/view"
        android:layout_width="120dp"
        android:layout_height="73dp"
        android:background="@drawable/login_white_backg5"
        app:layout_constraintBottom_toBottomOf="@+id/t3"
        app:layout_constraintEnd_toEndOf="@+id/t3"
        app:layout_constraintStart_toStartOf="@+id/t3"
        app:layout_constraintTop_toTopOf="@+id/t3"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/textView9"
        android:layout_width="90dp"
        android:layout_height="45dp"
        android:fontFamily="@font/hammersmith_one"
        android:hint="Tree 3"
        android:gravity="center"
        android:inputType="text"
        android:textAlignment="center"
        android:textAllCaps="true"
        android:textColor="#000000"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/t3"
        app:layout_constraintEnd_toEndOf="@+id/t3"
        app:layout_constraintStart_toStartOf="@+id/t3"
        app:layout_constraintTop_toTopOf="@+id/t3"
        app:layout_constraintVertical_bias="0.514" />

    <ImageButton
        android:id="@+id/t4"
        android:layout_width="161dp"
        android:layout_height="127dp"
        android:layout_marginBottom="16dp"
        android:background="@drawable/btn4"
        android:contentDescription="CERCOSPORA"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toTopOf="@+id/t6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.657"
        app:layout_constraintStart_toEndOf="@+id/t3" />

    <View
        android:id="@+id/view4"
        android:layout_width="120dp"
        android:layout_height="73dp"
        android:background="@drawable/login_white_backg5"
        app:layout_constraintBottom_toBottomOf="@+id/t4"
        app:layout_constraintEnd_toEndOf="@+id/t4"
        app:layout_constraintStart_toStartOf="@+id/t4"
        app:layout_constraintTop_toTopOf="@+id/t4"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/textView10"
        android:layout_width="90dp"
        android:layout_height="45dp"
        android:fontFamily="@font/hammersmith_one"
        android:hint="Tree 4"
        android:gravity="center"
        android:inputType="text"
        android:textAlignment="center"
        android:textAllCaps="true"
        android:textColor="#000000"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/t4"
        app:layout_constraintEnd_toEndOf="@+id/t4"
        app:layout_constraintStart_toStartOf="@+id/t4"
        app:layout_constraintTop_toTopOf="@+id/t4"
        app:layout_constraintVertical_bias="0.514" />

    <ImageButton
        android:id="@+id/t5"
        android:layout_width="161dp"
        android:layout_height="127dp"
        android:layout_marginBottom="160dp"
        android:background="@drawable/btn5"
        android:contentDescription="CERCOSPORA"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.064"
        app:layout_constraintStart_toStartOf="parent" />

    <View
        android:layout_width="120dp"
        android:layout_height="73dp"
        android:background="@drawable/login_white_backg5"
        app:layout_constraintBottom_toBottomOf="@+id/t5"
        app:layout_constraintEnd_toEndOf="@+id/t5"
        app:layout_constraintStart_toStartOf="@+id/t5"
        app:layout_constraintTop_toTopOf="@+id/t5"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/textView11"
        android:layout_width="90dp"
        android:layout_height="45dp"
        android:fontFamily="@font/hammersmith_one"
        android:hint="Tree 5"
        android:gravity="center"
        android:inputType="text"
        android:textAlignment="center"
        android:textAllCaps="true"
        android:textColor="#000000"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/t5"
        app:layout_constraintEnd_toEndOf="@+id/t5"
        app:layout_constraintStart_toStartOf="@+id/t5"
        app:layout_constraintTop_toTopOf="@+id/t5"
        app:layout_constraintVertical_bias="0.514" />


    <ImageButton
        android:id="@+id/t6"
        android:layout_width="161dp"
        android:layout_height="127dp"
        android:layout_marginBottom="80dp"
        android:background="@drawable/btn6"
        android:contentDescription="CERCOSPORA"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toTopOf="@+id/imageView2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.657"
        app:layout_constraintStart_toEndOf="@+id/t5" />

    <View
        android:layout_width="120dp"
        android:layout_height="73dp"
        android:background="@drawable/login_white_backg5"
        app:layout_constraintBottom_toBottomOf="@+id/t6"
        app:layout_constraintEnd_toEndOf="@+id/t6"
        app:layout_constraintStart_toStartOf="@+id/t6"
        app:layout_constraintTop_toTopOf="@+id/t6"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/textView12"
        android:layout_width="90dp"
        android:layout_height="45dp"
        android:fontFamily="@font/hammersmith_one"
        android:hint="Tree 6"
        android:gravity="center"
        android:inputType="text"
        android:textAlignment="center"
        android:textAllCaps="true"
        android:textColor="#000000"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/t6"
        app:layout_constraintEnd_toEndOf="@+id/t6"
        app:layout_constraintStart_toStartOf="@+id/t6"
        app:layout_constraintTop_toTopOf="@+id/t6"
        app:layout_constraintVertical_bias="0.514" />


    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="match_parent"
        android:layout_height="76dp"
        android:background="@drawable/login_white_backg7"
        app:layout_constraintBottom_toBottomOf="parent"
        tools:ignore="MissingConstraints" />


    <LinearLayout
        android:id="@+id/linearLayout6"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="16dp"
        android:gravity="center"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView2"
        app:layout_constraintVertical_bias="1.0"
        tools:ignore="MissingConstraints">

        <ImageButton
            android:id="@+id/home"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginStart="20dp"
            android:background="@drawable/button_selector" />

        <ImageButton
            android:id="@+id/leaf"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginStart="20dp"
            android:background="@drawable/button_selector2"

            />

        <ImageButton
            android:id="@+id/cam"
            android:layout_width="50dp"
            android:layout_height="48dp"
            android:layout_marginStart="20dp"
            android:background="@drawable/button_selector3"

            />


        <ImageButton
            android:id="@+id/history"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginStart="20dp"
            android:background="@drawable/button_selector4"
            android:backgroundTint="#017550" />

        <ImageButton
            android:id="@+id/cal"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginStart="20dp"
            android:background="@drawable/button_selector5" />

    </LinearLayout>


</androidx.constraintlayout.widget.ConstraintLayout>